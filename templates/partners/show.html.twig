{% extends 'base.html.twig' %}

{% block title %}Partners{% endblock %}

{% block body %}
    <h1>Partners</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ partner.id }}</td>
            </tr>
            <tr>
                <th>Est active</th>
                <td>{{ partner.isActive ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ partner.slug }}</td>
            </tr>
            <tr>
                <th>Modules</th>
                <td>
                    {% if partner.Modules %}
                    {% for module in partner.Modules %}
                    {{module}}
                    {% endfor %}
                    {% endif %}
                </td>
            </tr>
           
            
        </tbody>
    </table>

    <a href="{{ path('app_partners_index') }}">back to list</a>

    <a href="{{ path('app_partners_edit', {'id': partner.id}) }}">edit</a>

    {{ include('partners/_delete_form.html.twig') }}
{% endblock %}
